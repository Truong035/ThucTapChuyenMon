
@{
    ViewBag.Title = "Đánh Gía Kết Qủa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section scripts{
    <script>
        var item = $('#danhgia');
        item.addClass("active");
    </script>

}

<div class="col-md-11 card container"  style="margin-top:0px">
        <div class="col-md-6 text-center">
            <div class="form-group">
                <select class="col-md-8 form-control">
                    <option > sssss</option>
                    <option > sssss</option>
                    <option > sssss</option>
                </select>

            </div>
        </div>


    <div class="text-center">
        <div class="stats">
            <h4 class="card-title">  Biểu Đồ Đánh Gía Quán Trình Học Tập </h4>
        </div>
    </div>
    <div class=" card-chart"> 
        <div class="card-header card-header-tabs">
            <h4 class="card-title text-center">ssss </h4>
        </div>
        <div class="card-header card-header-info">
            <br />
            <div class="ct-chart" id="dailySalesChart"></div>
        </div>
        <div class="card-body">
            <h4 class="card-title">Nhận Xét</h4>
            <p class="card-category">
                <span class="text-success"><i class="fa fa-long-arrow-up"></i> 55% </span> increase in today sales.
            </p>
        </div>
        <div class="card-footer">
            <div class="stats">
                <i class="material-icons"> access_time</i> updated 4 minutes ago
            </div>
        </div>
    </div>
</div>